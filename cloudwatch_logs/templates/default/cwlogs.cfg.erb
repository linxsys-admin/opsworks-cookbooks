[general]
state_file = /var/awslogs/state/agent-state

[/var/log/apache2/wp_default-error.log]
file = /var/log/apache2/wp_default-error.log
log_group_name = PROD_PHP_logs
log_stream_name = {hostname}
buffer_duration = 5000
initial_position = end_of_file
datetime_format = %a %b %d %H:%M:%S.%f %Y

[/srv/www/wp_default/current/wp-content/plugins/wp-ses/log/wpses.log]
file = /srv/www/wp_default/current/wp-content/plugins/wp-ses/log/wpses.log
log_group_name = ses-logs
log_stream_name = wp-ses-plugin
buffer_duration = 5000
initial_position = start_of_file
datetime_format = %s
multi_line_start_pattern = {datetime_format}